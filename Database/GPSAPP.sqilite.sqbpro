CREATE TABLE IF NOT EXISTS &quot;APITokens_New&quot; (
    &quot;APITokenID&quot; INTEGER PRIMARY KEY,
    &quot;UserID&quot; INTEGER NOT NULL,
    &quot;APIService&quot; VARCHAR(50) NOT NULL,
    &quot;Token&quot; VARCHAR(255) NOT NULL,
    &quot;StravaAPIToken&quot; VARCHAR(255),
    &quot;RideWithGPSAPIToken&quot; VARCHAR(255),
    -- Add more properties for other API tokens
    &quot;PopulationDensityAPIToken&quot; VARCHAR(255),
    &quot;AccelerometerAPIToken&quot; VARCHAR(255),
    &quot;GoogleMapsAPIToken&quot; VARCHAR(255),
    &quot;GPSLoggerAPIToken&quot; VARCHAR(255),
    &quot;StravaTokenExpiration&quot; DATETIME,
    &quot;RideWithGPSTokenExpiration&quot; DATETIME,
    -- Add more expiration properties for other tokens
    FOREIGN KEY(&quot;UserID&quot;) REFERENCES &quot;User&quot;(&quot;UserID&quot;)
);

-- Copy data from the old table to the new table, explicitly specifying columns
