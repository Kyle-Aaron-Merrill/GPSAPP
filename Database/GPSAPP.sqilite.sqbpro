FROM &quot;APITokens&quot;;

-- Drop the old table
DROP TABLE &quot;APITokens&quot;;

-- Rename the new table to the original table name
ALTER TABLE &quot;APITokens_New&quot; RENAME TO &quot;APITokens&quot;;
